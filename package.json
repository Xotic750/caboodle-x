{
  "name": "caboodle-x",
  "version": "2.0.0",
  "description": "A collection of modern utils.",
  "homepage": "https://github.com/Xotic750/caboodle-x.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/Xotic750/caboodle-x.git"
  },
  "bugs": {
    "url": "https://github.com/Xotic750/caboodle-x/issues"
  },
  "files": [
    "dist",
    "src"
  ],
  "module": "dist/esm/caboodle-x.js",
  "main": "dist/caboodle-x.js",
  "jsdelivr": "dist/caboodle-x.min.js",
  "scripts": {
    "build:esm": "node_modules/.bin/mkdirp dist/esm && babel -s true --env-name esm src --out-dir dist/esm",
    "build:base": "node_modules/.bin/webpack --bail --progress --profile --colors",
    "build": "npm run build:esm && node_modules/.bin/cross-env NODE_ENV=production npm run build:base --",
    "build:dev": "npm run build:base --",
    "clean": "node_modules/.bin/rimraf dist && npm run clean:coverage",
    "clean:coverage": "node_modules/.bin/rimraf __tests__/coverage",
    "lint": "node_modules/.bin/eslint -f 'node_modules/eslint-friendly-formatter' --ext .js .",
    "lint-fix": "npm run lint -- --fix",
    "report": "npm run build -- --env.report",
    "report:dev": "npm run build:dev -- --env.report",
    "security": "npm audit",
    "security-fix": "npm run security -- fix",
    "start": "node_modules/.bin/nodemon --exec \\\\\\\"npm run build\\\\\\\" --watch src",
    "test": "npm run clean:coverage && node_modules/.bin/jest",
    "test:ci": "npm run test -- --ci --maxWorkers=2",
    "test:coverage": "npm run test -- --coverage"
  },
  "author": {
    "name": "Graham Fairweather",
    "email": "xotic750@gmail.com"
  },
  "copyright": "Copyright (c) 2018-present",
  "license": "MIT",
  "keywords": [
    "modern",
    "utils",
    "library",
    "module",
    "node",
    "browser"
  ],
  "dependencies": {
    "delay-promise-x": "^1.0.20",
    "is-boolean-object": "^1.0.0",
    "is-date-object": "^1.0.1",
    "is-number-object": "^1.0.3",
    "is-string": "^1.0.4",
    "is-symbol": "^1.0.2"
  },
