{
  "name": "caboodle-x",
  "version": "2.0.0",
  "description": "A collection of modern utils.",
  "homepage": "https://github.com/Xotic750/caboodle-x.git",
  "repository": {
    "type": "git",
    "url": "https://github.com/Xotic750/caboodle-x.git"
  },
  "bugs": {
    "url": "https://github.com/Xotic750/caboodle-x/issues"
  },
  "browser": "dist/caboodle-x.min.js",
  "main": "dist/caboodle-x.js",
  "module": "index.js",
  "scripts": {
    "clean:dist": "rimraf dist",
    "clean:docs": "rimraf docs",
    "clean:coverage": "rimraf __tests__/coverage",
    "clean": "npm run clean:dist && npm run clean:docs && npm run clean:coverage",
    "development": "npm run clean && NODE_ENV=development webpack --bail --config webpack.config.js --progress --profile --colors",
    "lint": "eslint -f 'node_modules/eslint-friendly-formatter' --ext .js,.vue,.json .",
    "lint-fix": "npm run lint -- --fix",
    "lint:css": "stylelint '**/*.{css,less,vue}'",
    "lint-fix:css": "npm run lint:css -- --fix",
    "lint:all": "npm run lint:css & npm run lint",
    "lint-fix:all": "npm run lint-fix:css & npm run lint-fix",
    "security": "npm audit",
    "security-fix": "npm run security -- fix",
    "prettier-check": "eslint --print-config . | eslint-config-prettier-check",
    "production": "npm run clean && NODE_ENV=production webpack --bail --config webpack.config.js --progress --profile --colors",
    "report": "npm run clean && NODE_ENV=production webpack --bail --config webpack.config.js --progress --profile --colors --env.report",
    "start": "nodemon --exec \"npm run production\" -e js,json,vue,gif --watch src --watch src/locales --watch src/images",
    "test": "jest",
    "test:ci": "npm run test -- --ci --maxWorkers=2",
    "test:coverage": "npm run test -- --coverage"
  },
  "author": {
    "name": "Graham Fairweather",
    "email": "xotic750@gmail.com"
  },
  "copyright": "Copyright (c) 2018-present",
  "license": "MIT",
  "keywords": [
    "modern",
    "utils",
    "library",
    "module",
    "node",
    "browser"
  ],
  "dependencies": {
    "delay-promise-x": "^1.0.0",
    "is-boolean-object": "^1.0.0",
    "is-date-object": "^1.0.1",
    "is-number-object": "^1.0.3",
    "is-string": "^1.0.4",
    "is-symbol": "^1.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/plugin-proposal-class-properties": "^7.3.4",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-export-default-from": "^7.2.0",
    "@babel/plugin-syntax-export-namespace-from": "^7.2.0",
    "@babel/plugin-syntax-jsx": "^7.2.0",
    "@babel/plugin-transform-property-mutators": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.3.4",
    "@babel/runtime": "^7.3.4",
    "@prorenata/eslint-config-vue": "^1.12.1",
    "babel-core": "^7.0.0-0",
    "babel-eslint": "^10.0.1",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^8.0.5",
    "babel-plugin-js-logger": "^1.0.17",
    "babel-plugin-lodash": "^3.3.4",
    "caniuse-lite": "^1.0.30000939",
    "cross-env": "^5.2.0",
    "eslint": "^5.15.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.11.0",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-compat": "^2.7.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-eslint-comments": "^3.1.1",
    "eslint-plugin-html": "^5.0.3",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-jsdoc": "^4.1.1",
    "eslint-plugin-json": "^1.4.0",
    "eslint-plugin-lodash": "^5.1.0",
    "eslint-plugin-no-use-extend-native": "^0.4.0",
    "eslint-plugin-prefer-object-spread": "^1.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-sort-class-members": "^1.4.0",
    "eslint-plugin-switch-case": "^1.1.2",
    "eslint-plugin-vue": "^5.2.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.1.0",
    "jest-cli": "^24.1.0",
    "jest-css": "^1.0.1",
    "jest-file": "^1.0.0",
    "js-logger": "^1.6.0",
    "jsdoc-to-markdown": "^4.0.1",
    "lodash": "^4.17.11",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "node-notifier": "^5.4.0",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3",
    "semver": "^5.6.0",
    "source-map-loader": "^0.2.4",
    "terser-webpack-plugin": "^1.2.3",
    "webpack": "^4.29.6",
    "webpack-bundle-analyzer": "^3.1.0",
    "webpack-cli": "^3.2.3",
    "webpack-merge": "^4.2.1"
  },
  "engines": {
    "node": ">=8.11.4",
    "npm": "6.4.1"
  },
  "browserslist": [
    "last 3 Chrome versions",
    "last 3 Firefox versions",
    "Safari >= 10",
    "Explorer >= 11",
    "Edge >= 12",
    "iOS >= 10",
    "Android >= 6"
  ]
}
