    "caniuse-lite": "^1.0.30000985",
    "chalk": "^2.4.2",
    "concurrently": "^4.1.1",
    "cross-env": "^5.2.0",
    "eslint": "=5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^2.2.1",
