    "eslint-plugin-sort-class-members": "^1.5.0",
    "eslint-plugin-switch-case": "^1.1.2",
    "eslint-plugin-vue": "^5.2.3",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.8.0",
    "jest-cli": "^24.8.0",
